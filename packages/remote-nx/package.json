  "dependencies": {
    "@vercel/remote": "workspace:*",
    "chalk": "^4.1.0",
    "nx-remotecache-custom": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@nx/workspace": "^16.3.2",
    "@types/yargs": "^17.0.24",
    "tsconfig": "workspace:*",
    "typescript": "5.1.3",
    "vitest": "^0.32.0"
  }
}
